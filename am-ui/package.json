{
  "name": "marketplace-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:webcomponents": "node build_scripts/install-webcomponents.js && node build_scripts/build-webcomponents.js && node build_scripts/package-webcomponents.js && node build_scripts/add-to-script-array.js",
    "prestart": "pnpm build:webcomponents",
    "prestart_ssl": "pnpm build:webcomponents",
    "start": "pnpm prestart && ng serve",
    "start_ssl": "pnpm prestart && ng serve --ssl true",
    "prebuild": "pnpm build:webcomponents",
    "build": "pnpm prebuild && ng build",
    "prebuild:prod": "node build_scripts/install-webcomponents.js && node build_scripts/build-webcomponents.js --prod && node build_scripts/package-webcomponents.js && node build_scripts/add-to-script-array.js",
    "build:prod": "pnpm prebuild:prod && ng build --configuration production",
    "test": "jest --coverage",
    "clear_jest": "jest --clearCache",
    "lint": "pnpm lint:angular && pnpm lint:typescript && pnpm lint:style",
    "lint:angular": "ng lint",
    "lint:typescript": "gts lint",
    "lint:style": "npx stylelint \"**/*.scss\"",
    "fix": "pnpm fix:angular && pnpm fix:typescript && pnpm fix:style",
    "fix:angular": "ng lint --fix",
    "fix:typescript": "gts fix",
    "fix:style": "npx stylelint \"**/*.scss\" --fix",
    "clean": "gts clean",
    "preinstall": "node build_scripts/install-webcomponents.js",
    "openapi-gen": "ng-openapi-gen --input ../../../api.yaml --output src/app/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.12",
    "@angular/common": "~13.3.12",
    "@angular/compiler": "~13.3.12",
    "@angular/core": "~13.3.12",
    "@angular/forms": "~13.3.12",
    "@angular/platform-browser": "~13.3.12",
    "@angular/platform-browser-dynamic": "~13.3.12",
    "@angular/router": "~13.3.12",
    "@azure/msal-angular": "^2.5.0",
    "@azure/msal-browser": "^2.32.0",
    "@azure/msal-common": "^6.4.0",
    "@ngneat/until-destroy": "^9.2.2",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/google.accounts": "^0.0.2",
    "angular-oauth2-oidc": "^13.0.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.2.3",
    "eslint": "^8.28.0",
    "fs-extra": "^10.1.0",
    "leaflet": "^1.9.3",
    "ngrx": "0.0.1-security",
    "ngx-logger": "^5.0.11",
    "pnpm": "^7.17.0",
    "rxjs": "^6.6.7",
    "swiper": "^8.4.5",
    "tslib": "^2.4.1",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.3.10",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.3.12",
    "@types/jest": "^27.5.2",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/experimental-utils": "^5.44.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "gts": "^3.1.1",
    "husky": "^8.0.2",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.2",
    "stylelint": "^14.15.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-node": "~9.0.0",
    "typescript": "~4.5.5"
  }
}
