{
  "name": "marketplace-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build:webcomponents": "node build_scripts/install-webcomponents.js && node build_scripts/build-webcomponents.js && node build_scripts/package-webcomponents.js && node build_scripts/add-to-script-array.js",
    "prestart": "npm run build:webcomponents",
    "prestart_ssl": "npm run build:webcomponents",
    "start": "ng serve",
    "start_ssl": "ng serve --ssl true",
    "prebuild": "npm run build:webcomponents",
    "build": "ng build",
    "prebuild:prod": "node build_scripts/install-webcomponents.js && node build_scripts/build-webcomponents.js --prod && node build_scripts/package-webcomponents.js && node build_scripts/add-to-script-array.js",
    "build:prod": "ng build --configuration production",
    "test": "jest --coverage",
    "clear_jest": "jest --clearCache",
    "lint": "npm run lint:angular && npm run lint:typescript && npm run lint:style",
    "lint:angular": "ng lint",
    "lint:typescript": "gts lint",
    "lint:style": "npx stylelint \"**/*.scss\"",
    "fix": "npm run fix:angular && npm run fix:typescript && npm run fix:style",
    "fix:angular": "ng lint --fix",
    "fix:typescript": "gts fix",
    "fix:style": "npx stylelint \"**/*.scss\" --fix",
    "clean": "gts clean",
    "preinstall": "node build_scripts/install-webcomponents.js",
    "openapi-gen": "ng-openapi-gen --input ../../../api.yaml --output src/app/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.2",
    "@angular/common": "~13.1.2",
    "@angular/compiler": "~13.1.2",
    "@angular/core": "~13.1.2",
    "@angular/forms": "~13.1.2",
    "@angular/platform-browser": "~13.1.2",
    "@angular/platform-browser-dynamic": "~13.1.2",
    "@angular/router": "~13.1.2",
    "@azure/msal-angular": "^2.1.0",
    "@azure/msal-browser": "^2.21.0",
    "@azure/msal-common": "^6.0.0",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/google.accounts": "^0.0.2",
    "angular-oauth2-oidc": "^13.0.1",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "eslint": "^8.6.0",
    "leaflet": "^1.8.0",
    "ngrx": "0.0.1-security",
    "ngx-logger": "^5.0.4",
    "rxjs": "^6.6.7",
    "swiper": "^8.4.2",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^13.1.4",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/experimental-utils": "^5.10.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^8.2.0",
    "gts": "^3.1.0",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.0.1",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.5.4"
  }
}
