{
  "name": "search-webcomponent",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run openapi-gen && ng serve",
    "build": "npm run openapi-gen && ng build --output-hashing=none",
    "watch": "npm run openapi-gen && ng build --watch --configuration development",
    "test": "jest --coverage",
    "prepackage": "npm run build",
    "package": "node package-script.js",
    "export": "npm run build && npm run package",
    "lint": "npm run lint:angular && npm run lint:typescript && npm run lint:style",
    "lint:angular": "ng lint",
    "lint:typescript": "gts lint",
    "lint:style": "npx stylelint \"**/*.scss\"",
    "fix": "npm run fix:angular && npm run fix:typescript && npm run fix:style",
    "fix:angular": "ng lint --fix",
    "fix:typescript": "gts fix",
    "fix:style": "npx stylelint \"**/*.scss\" --fix",
    "openapi-gen": "ng-openapi-gen --input ../../../marketplace-service/src/main/resources/api.yaml --output src/app/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "~13.1.0",
    "@angular/core": "~13.1.0",
    "@angular/elements": "^13.1.3",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ngneat/tailwind": "^7.0.3",
    "@ngrx/effects": "^13.0.2",
    "@ngrx/store": "^13.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@openapitools/openapi-generator-cli": "^2.4.26",
    "ng-openapi-gen": "^0.20.0",
    "ngx-logger": "^5.0.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "tw-elements": "^1.0.0-alpha10",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.3",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.1.3",
    "@angular/compiler-cli": "~13.1.0",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/experimental-utils": "^5.10.0",
    "@typescript-eslint/parser": "5.3.0",
    "concat": "^1.0.3",
    "eslint": "^8.2.0",
    "fs-extra": "^10.0.0",
    "gts": "^3.1.0",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.1.0",
    "postcss": "^8.4.6",
    "stylelint": "^14.2.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "tailwindcss": "latest",
    "ts-node": "~8.3.0",
    "typescript": "~4.5.4"
  }
}
